{
    "name": "vehicle-platform-ccp",
    "version": "1.0.0",
    "client": {
        "organization": "${ORG}",
        "connection": {
            "timeout": {
                "peer": {
                    "endorser": "300"
                }
            }
        }
    },
    "organizations": {
        "${ORG}": {
            "mspid": "${ORGMSP}MSP",
            "peers": ["peer0.${ORG}.vtan.com"],
            "certificateAuthorities": ["ca.${ORG}.vtan.com"]
        }
    },
    "peers": {
        "peer0.${ORG}.vtan.com": {
            "url": "grpcs://localhost:${P0PORT}",
            "tlsCACerts": {
                "pem": "${PEERPEM}"
            },
            "grpcOptions": {
                "ssl-target-name-override": "peer0.${ORG}.vtan.com",
                "hostnameOverride": "peer0.${ORG}.vtan.com"
            }
        }
    },
    "certificateAuthorities": {
        "ca.${ORG}.vtan.com": {
            "url": "https://localhost:${CAPORT}",
            "caName": "ca-${ORG}",
            "tlsCACerts": {
                "pem": "${CAPEM}"
            },
            "httpOptions": {
                "verify": false
            }
        }
    }
}
